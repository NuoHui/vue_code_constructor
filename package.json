    "access": "public"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "standard": {
    "globals": [],
    "ignore": []
  },
  "browserslist": [
    "> 1%",
    "last 10 Chrome versions",
    "last 5 Firefox versions",
    "Safari >= 6",
    "ie > 8",
    "android >= 4.4",
    "ios >= 8"
  ]
}
